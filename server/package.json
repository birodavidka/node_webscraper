{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "test": "jest",
    "watch": "nodemon --watch src --ext ts,js --exec \"npm run dev\"",
    "test:scraper": "ts-node-dev --respawn --transpile-only ./test/testScraper.ts",
    "test:bright": "ts-node-dev --respawn --transpile-only ./test/testBright.ts",
    "test:http": "ts-node-dev --respawn --transpile-only ./test/testHttp.ts",
    "scrape:once": "ts-node --transpile-only scripts/runScraper.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.10.0",
    "axios-retry": "^4.5.0",
    "body-parser": "^2.2.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "puppeteer": "^24.10.0",
    "telegraf": "^4.16.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.57"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
